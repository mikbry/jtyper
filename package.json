{
  "name": "jtyper",
  "version": "0.1.0",
  "description": "Javascript notebook running in browser",
  "main": "dist/index.js",
  "repository": "https://github.com/mikbry/jtyper.git",
  "homepage": "https://github.com/mikbry/jtyper",
  "author": "Mik <mik@mikbry.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf build && mkdir build",
    "build": "yarn clean & cross-env NODE_ENV=production rollup -c",
    "start": "webpack-dev-server --env.NOTEBOOK_PATH=notebooks --mode development --open --hot --port 9010",
    "lint": "$(yarn bin)/eslint src --ext .ts",
    "test": "cross-env NODE_ENV=test  jest",
    "test:watch": "jest --watch",
    "coverage": "cross-env NODE_ENV=test  jest --coverage --colors",
    "report-coverage": "$(yarn bin)/nyc report --reporter=text-lcov > coverage.lcov",
    "prepublishOnly": "yarn build",
    "netlify": "node_modules/.bin/rollup -c"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@rollup/plugin-url": "^4.0.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@types/codemirror": "^0.0.91",
    "@types/jest": "^25.2.1",
    "@types/markdown-it": "^10.0.0",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.5.2",
    "dotenv-webpack": "^1.8.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.1",
    "jest": "^25.4.0",
    "jest-styled-components": "^7.0.2",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "react-refresh": "^0.7.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.1",
    "rollup": "^1.30.1",
    "rollup-plugin-commonjs-alternate": "^0.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot-css": "^0.0.6",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-react-refresh": "^0.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^5.0.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    },
    "commitlint": {
      "extends": [
        "@commitlint/config-conventional"
      ]
    }
  },
  "dependencies": {
    "@styled-icons/boxicons-regular": "^10.0.0",
    "@styled-icons/fa-regular": "^10.0.0",
    "@styled-icons/feather": "^10.0.0",
    "@styled-icons/icomoon": "^10.0.0",
    "@styled-icons/styled-icon": "^10.0.0",
    "acorn": "^7.1.1",
    "codemirror": "^5.53.2",
    "markdown-it": "^10.0.0",
    "react": "^16.12.0",
    "react-codemirror2": "^7.1.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router": "^6.0.0-alpha.4",
    "react-router-dom": "^6.0.0-alpha.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.0"
  }
}
