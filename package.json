{
  "name": "jtyper",
  "version": "0.1.0",
  "description": "Javascript notebook running in browser",
  "main": "dist/index.js",
  "repository": "https://github.com/mikbry/jtyper.git",
  "homepage": "https://github.com/mikbry/jtyper",
  "author": "Mik <mik@mikbry.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf build && mkdir build",
    "build": "yarn clean && cross-env NODE_ENV=production rollup -c",
    "start": "webpack-dev-server --env.NOTEBOOK_PATH=notebooks --mode development --open --hot --port 9010",
    "lint": "$(yarn bin)/eslint src --ext .ts",
    "test": "cross-env NODE_ENV=test  jest",
    "test:watch": "jest --watch",
    "coverage": "cross-env NODE_ENV=test  jest --coverage --colors",
    "report-coverage": "$(yarn bin)/nyc report --reporter=text-lcov > coverage.lcov",
    "prepublishOnly": "yarn build",
    "prod": "yarn clean && rollup -c && cp -r public/notebooks build && rm build/404.html && cp public/_redirects build"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/codemirror": "^0.0.97",
    "@types/jest": "^26.0.10",
    "@types/markdown-it": "^10.0.2",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.0",
    "css-loader": "^4.2.1",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.1",
    "jest": "^26.4.0",
    "jest-styled-components": "^7.0.2",
    "prettier": "^2.0.5",
    "raw-loader": "^4.0.0",
    "react-refresh": "^0.8.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.1",
    "rollup": "^2.26.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot-css": "^0.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-react-refresh": "^0.0.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^6.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    },
    "commitlint": {
      "extends": [
        "@commitlint/config-conventional"
      ]
    }
  },
  "dependencies": {
    "@styled-icons/boxicons-regular": "^10.15.1",
    "@styled-icons/fa-regular": "^10.15.1",
    "@styled-icons/feather": "^10.15.1",
    "@styled-icons/icomoon": "^10.15.1",
    "@styled-icons/styled-icon": "^10.6.2",
    "acorn": "^8.0.1",
    "codemirror": "^5.58.2",
    "history": "^5.0.0",
    "markdown-it": "^11.0.0",
    "react": "^16.12.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1"
  }
}
